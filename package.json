{
  "name": "react-css-components",
  "version": "1.0.1",
  "description": "Define styled React components using CSS based module format",
  "main": "lib/index.js",
  "webpackLoader": "webpack.js",
  "babel": {
    "plugins": [
      "ast-literal"
    ],
    "presets": [
      "prometheusresearch"
    ]
  },
  "eslintConfig": {
    "extends": "prometheusresearch",
    "env": {
      "mocha": true
    },
    "globals": {
      "expr": true,
      "stmt": true
    }
  },
  "scripts": {
    "test": "make lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreypopp/react-css-components.git"
  },
  "keywords": [
    "react",
    "react-dom",
    "css",
    "postcss",
    "postcss-plugin",
    "webpack",
    "webpack-loader"
  ],
  "author": "Andrey Popp <8mayday@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreypopp/react-css-components/issues"
  },
  "homepage": "https://github.com/andreypopp/react-css-components#readme",
  "peerDependencies": {
    "style-loader": "*",
    "css-loader": "*"
  },
  "dependencies": {
    "babel-generator": "^6.9.0",
    "babel-plugin-ast-literal": "^0.5.4",
    "babel-traverse": "^6.7.6",
    "babel-types": "^6.7.2",
    "babylon": "^6.7.0",
    "invariant": "^2.2.1",
    "loader-utils": "^0.2.14",
    "postcss": "^5.0.19",
    "postcss-selector-parser": "^1.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.7.6",
    "babel-generator": "^6.7.5",
    "babel-preset-prometheusresearch": "^0.1.0",
    "doctoc": "^1.1.1",
    "eslint": "^2.7.0",
    "eslint-config-prometheusresearch": "^0.2.0",
    "eslint-plugin-react": "^4.3.0",
    "flow-bin": "^0.26.0",
    "mocha": "^2.4.5"
  }
}
